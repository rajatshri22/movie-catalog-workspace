{
    "name": "frontend",
    "version": "1.0.0",
    "description": "Frontend for the Movie Catalog application",
    "scripts": {
      "start": "webpack serve --mode development", 
      "build": "webpack --mode production",  
      "test": "jest",
      "lint": "eslint .",  
      "prettify": "prettier --write '**/*.js'" 
    },
    "dependencies": {
      "react": "^17.0.2", 
      "react-dom": "^17.0.2"  
    },
    "devDependencies": {
      "webpack": "^5.0.0",  
      "webpack-cli": "^4.0.0", 
      "eslint": "^7.0.0",
      "jest": "^27.0.0", 
      "prettier": "^2.0.0" 
    },
    "eslintConfig": {
      "extends": "eslint:recommended",
      "env": {
        "browser": true,
        "es2021": true
      },
      "rules": {
        "no-unused-vars": "warn",
        "no-console": "off"
      }
    },
    "jest": {
      "testEnvironment": "jsdom"
    }
  }
  